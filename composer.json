{
    "name": "deltstl/air-choice-one",
    "description": "Air Choice One Drupal 8 project",
    "type": "project",
    "license": "proprietary",
    "authors": [
        {
            "name": "Daniel Korte",
            "email": "daniel@danielkorte.com",
            "homepage": "https://www.danielkorte.com",
            "role": "Web Developer"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "require": {
        "php": ">=5.6",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "defuse/php-encryption": "^2.2",
        "drupal/address": "^1.9",
        "drupal/admin_toolbar": "^2.0",
        "drupal/antibot": "^1.2",
        "drupal/backup_migrate": "^4.1",
        "drupal/better_formats": "1.x-dev@dev",
        "drupal/bfd": "^2.61",
        "drupal/change_pwd_page": "^1.0",
        "drupal/colorbox": "^1.7",
        "drupal/config_filter": "^1.4",
        "drupal/config_split": "^1.4",
        "drupal/context": "^4.0@beta",
        "drupal/core-composer-scaffold": "^8.9",
        "drupal/core-recommended": "^8.9",
        "drupal/crop": "^1.5",
        "drupal/diff": "^1.0@RC",
        "drupal/editor_advanced_link": "^1.4",
        "drupal/editor_file": "^1.4",
        "drupal/embed": "^1.0",
        "drupal/empty_page": "^3.0",
        "drupal/entity": "^1.2",
        "drupal/entity_browser": "^2.2",
        "drupal/field_group": "^3.0",
        "drupal/field_permissions": "^1.1",
        "drupal/focal_point": "^1.2",
        "drupal/fontawesome": "^2.18",
        "drupal/http_client_manager": "^2.3",
        "drupal/iframe": "^1.9",
        "drupal/imageapi_optimize": "^2.0@alpha",
        "drupal/imageapi_optimize_resmushit": "^1.0@beta",
        "drupal/imageapi_optimize_tinypng": "^1.0@beta",
        "drupal/imagemagick": "^3.1",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/jquery_ui": "^1.4",
        "drupal/jquery_ui_accordion": "^1.1",
        "drupal/linkit": "^4.3",
        "drupal/login_history": "^1.0",
        "drupal/login_security": "^1.5",
        "drupal/login_tracker": "^1.4",
        "drupal/mailsystem": "^4.1",
        "drupal/media_entity_browser": "^2.0",
        "drupal/menu_admin_per_menu": "^1.0",
        "drupal/menu_link_attributes": "^1.0",
        "drupal/metatag": "^1.10",
        "drupal/minifyhtml": "^1.8",
        "drupal/paragraphs": "^1.9",
        "drupal/password_policy": "^3.0@alpha",
        "drupal/pathauto": "^1.5",
        "drupal/pathologic": "^1.0@alpha",
        "drupal/profile": "^1.2",
        "drupal/rabbit_hole": "^1.0@beta",
        "drupal/redirect": "^1.4",
        "drupal/redirect_after_login": "^2.7",
        "drupal/roleassign": "^1.0@alpha",
        "drupal/schema_metatag": "^1.4",
        "drupal/simple_sitemap": "^3.3",
        "drupal/smtp": "^1.0",
        "drupal/spamspan": "^1.0",
        "drupal/superfish": "^1.4",
        "drupal/swiftmailer": "^2.0",
        "drupal/tab_toolbar": "^1.0@beta",
        "drupal/twig_tweak": "^2.9",
        "drupal/user_current_paths": "^2.0",
        "drupal/views_accordion": "^2.0",
        "drupal/we_megamenu": "^1.11",
        "drupal/webform": "^5.4",
        "drush/drush": "^10.4",
        "stripe/stripe-php": "^7.75",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "require-dev": {
        "behat/behat": "^3.5",
        "drupal/core-dev": "^8.9",
        "drupal/devel": "^2.1",
        "drupal/drupal-extension": "^3.4"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "bin-dir": "bin/",
        "platform": {
            "php": "7.1.31"
        }
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "nuke": "rm -rf bin vendor public_html/core public_html/libraries public_html/modules/contrib public_html/profiles/contrib"
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "enable-patching": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "installer-paths": {
            "public_html/core": ["type:drupal-core"],
            "public_html/libraries/{$name}": ["type:drupal-library"],
            "public_html/modules/contrib/{$name}": ["type:drupal-module"],
            "public_html/profiles/contrib/{$name}": ["type:drupal-profile"],
            "public_html/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "public_html/"
            },
            "file-mapping": {
                "[project-root]/.editorconfig": {
                    "append": "assets/.editorconfig"
                },
                "[project-root]/.gitattributes": {
                    "prepend": "assets/.gitattributes"
                },
                "[web-root]/.htaccess": {
                    "prepend": "assets/htaccess-prepend.conf",
                    "append": "assets/htaccess-append.conf"
                },
                "[web-root]/robots.txt": {
                    "append": "assets/robots.txt"
                }
            }
        }
    }
}
