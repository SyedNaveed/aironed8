{#
/**
 * @file
 * Display an iframe with alterable components.
 */
#}
<span class="intrinsic intrinsic--16x9">
<iframe{{ attributes.addClass('intrinsic__item') }}{% if url is not empty %} src="{{ url }}{% if query is not empty %}?{{ query | url_encode }}{% endif %}{% if fragment is not empty %}#{{ fragment }}{% endif %}"{% endif %}></iframe>
</span>
