{#
/**
 * @file
 * Theme override for a flight.
 */
#}
<div class="book_ticket_body bt_outbound">
  <h4>{{ title }}</h4>

  <div class="row rnp">
    <div class="col-xs-3">
      <b>{{ 'Date:'|t }}</b>
    </div>
    <div class="col-xs-9">
      {{ date }}
    </div>
  </div>

  <div class="row rnp">
    <div class="col-xs-3">
      <b>{{ 'From:'|t }}</b>
    </div>
    <div class="col-xs-9">
      {{ from }}
    </div>
  </div>

  <div class="row rnp">
    <div class="col-xs-3">
      <b>{{ 'To:'|t }}</b>
    </div>
    <div class="col-xs-9">
       {{ to }}
    </div>
  </div>
</div>
