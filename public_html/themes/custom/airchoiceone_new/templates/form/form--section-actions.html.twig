{#
/**
 * @file
 * Default theme implementation for a 'form' element.
 *
 * Available variables
 * - attributes: A list of HTML attributes for the wrapper element.
 * - children: The child elements of the form.
 *
 * @see template_preprocess_form()
 *
 * @ingroup themeable
 */
#}
{{ element|without('actions') }}
<div class="section__actions">
  <ul class="block-list horizontal-list">
    <li>{{ element.actions.button1 }}</li>
    {% if element.actions.button2 %}
      <li>{{ element.actions.button2 }}</li>
    {% endif %}
    {% if element.actions.button3 %}
      <li>{{ element.actions.button3 }}</li>
    {% endif %}
    {% if element.actions.button4 %}
      <li>{{ element.actions.button4 }}</li>
    {% endif %}
  </ul>
</div>
