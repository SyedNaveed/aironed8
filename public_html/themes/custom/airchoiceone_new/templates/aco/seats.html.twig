{#
/**
 * @file
 * Theme override for seats.
 */
#}
{% for flight in flights %}
  <div data-tab="{{ loop.index0 }}" class="booking_seats">
    <img src="/themes/custom/airchoiceone_new/assets/image/seats-new.png" alt="{{ 'Seat assignment'|t }}">
    <div class="seats">
      <ul class="block-list">
        {% for seat in flight.seats %}
          {{ seat }}
        {% endfor %}
      </ul>
    </div>
  </div>
{% endfor %}
